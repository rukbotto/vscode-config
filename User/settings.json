{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "editor.minimap.enabled": false,
    "workbench.activityBar.visible": true,
    "editor.formatOnSave": true,
    "editor.rulers": [
        80,
        120
    ],
    "git.autofetch": true,
    "window.zoomLevel": 0,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "emmet.includeLanguages": {
        "liquid": "html"
    },
    "editor.tabSize": 2,
    "[python]": {
        "editor.tabSize": 4
    },
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "python.venvFolders": [
        "envs",
        ".pyenv",
        ".direnv",
        ".virtualenv",
        ".virtualenvs"
    ],
    "terminal.external.osxExec": "iTerm.app",
    "files.associations": {
        "Vagrantfile": "ruby",
        ".env*": "dotenv"
    },
    "files.exclude": {
        "**/.git": false,
        "**/.git/objects": true,
        "**/.git/subtree-cache": true
    },
    "[shellscript]": {
        "editor.insertSpaces": false,
        "editor.tabSize": 4,
        "files.eol": "\n"
    },
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": false,
        "editor.rulers": []
    },
    "workbench.colorTheme": "Base16 Github",
    "workbench.colorCustomizations": {
        "[Base16 Github]": {
            "breadcrumb.focusForeground": "#A71D5D",
            "breadcrumb.activeSelectionForeground": "#A71D5D",
            "button.background": "#795DA3",
            "button.hoverBackground": "#C8C8FA",
            "editorWidget.background": "#F5F5F5",
            "icon.foreground": "#333333",
            "inputValidation.warningForeground": "#FFFFFF",
            "list.activeSelectionBackground": "#E8E8E8",
            "list.inactiveSelectionBackground": "#E8E8E8",
            "list.hoverBackground": "#E8E8E8",
            "selection.background": "#C8C8FA",
            "sideBarSectionHeader.background": "#C8C8FA"
        }
    }
}
